{
  "name": "@willsoto/casbin-objection-adapter",
  "version": "0.2.0",
  "description": "",
  "keywords": [
    "casbin",
    "objection",
    "knex",
    "casbin-database",
    "casbin-adapter"
  ],
  "repository": {
    "type": "git",
    "url": "github:willsoto/casbin-objection-adapter"
  },
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "exports": {
    "import": "./dist/esm.js",
    "main": "./dist/cjs.js"
  },
  "main": "./dist/cjs.js",
  "typings": "./typings/index.d.ts",
  "files": [
    "dist",
    "src",
    "typings"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup --config rollup.config.js",
    "postbuild": "npm run typings",
    "clean": "rimraf dist typings coverage",
    "lint": "eslint src test --ext ts --fix",
    "prerelease": "npm run build",
    "release": "standard-version --sign",
    "test": "jest",
    "pretest:coverage": "rimraf coverage",
    "test:coverage": "jest --coverage",
    "pretypings": "rimraf typings",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "npm run lint",
      "prettier --write"
    ],
    "README.md": [
      "markdown-toc -i",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.9.6",
    "@babel/preset-env": "~7.9.6",
    "@babel/preset-typescript": "~7.9.0",
    "@commitlint/cli": "~8.3.5",
    "@commitlint/config-conventional": "~8.3.4",
    "@rollup/plugin-typescript": "~4.1.1",
    "@sindresorhus/slugify": "~1.0.0",
    "@types/jest": "~25.2.3",
    "@types/node": "~14.0.1",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "babel-jest": "~26.0.1",
    "casbin": "~5.0.0",
    "eslint": "~7.0.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.13.1",
    "husky": "~4.2.5",
    "jest": "~26.0.1",
    "knex": "~0.21.1",
    "lint-staged": "~10.2.4",
    "markdown-toc": "~1.2.0",
    "objection": "~2.1.3",
    "prettier": "~2.0.5",
    "prettier-plugin-organize-imports": "~1.0.4",
    "prettier-plugin-packagejson": "~2.2.3",
    "rimraf": "~3.0.2",
    "rollup": "~2.10.3",
    "rollup-plugin-filesize": "~9.0.0",
    "sqlite3": "~4.2.0",
    "standard-version": "~8.0.0",
    "tslib": "~2.0.0",
    "typescript": "~3.9.2",
    "winston": "~3.2.1"
  },
  "peerDependencies": {
    "casbin": "^5.0.0",
    "knex": "^0.21.1",
    "objection": "^2.1.3"
  },
  "volta": {
    "node": "14.2.0"
  }
}
